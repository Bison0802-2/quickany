{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quick-any",
  "title": "Quick Any",
  "description": "open any application page quickly.",
  "icon": "slack-icon.png",
  "author": "kosuke_umemura",
  "owner": "AKARI",
  "license": "MIT",
  "commands": [
    {
      "name": "notionRegister",
      "title": "Register Page",
      "subtitle": "Quick Notion",
      "description": "Register Notion Page",
      "mode": "view",
      "preferences": [
        {
          "title": "Default Workspace ID",
          "name": "workspace",
          "description": "default workspace ID for registering a page.",
          "type": "textfield",
          "defaultValue": "",
          "required": false
        }
      ],
      "icon": "notion-icon.png"
    },
    {
      "name": "notionOpen",
      "title": "Open Page",
      "subtitle": "Quick Notion",
      "description": "Open Registered Page",
      "mode": "view",
      "icon": "notion-icon.png"
    },
    {
      "name": "slackRegister",
      "title": "Register Channel",
      "subtitle": "Quick Slack",
      "description": "Register Slack Channel",
      "mode": "view",
      "preferences": [
        {
          "title": "Default Team ID",
          "name": "team",
          "description": "default team ID for registering a channel.",
          "type": "textfield",
          "defaultValue": "",
          "required": false
        }
      ]
    },
    {
      "name": "slackOpen",
      "title": "Open Channel",
      "subtitle": "Quick Slack",
      "description": "Open Registered Channel",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1",
    "@types/fs-extra": "^11.0.4",
    "change-case": "^5.4.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
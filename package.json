{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quick-slack",
  "title": "quick slack",
  "description": "open slack channel quickly.",
  "icon": "command-icon.png",
  "author": "kosuke_umemura",
  "license": "MIT",
  "commands": [
    {
      "name": "register",
      "title": "Register Channel",
      "subtitle": "Quick Slack",
      "description": "Register Slack Channel",
      "mode": "view",
      "preferences": [
        {
          "title": "Default Team ID",
          "name": "defaultTeamID",
          "description": "default team ID for registering a channel.",
          "type": "textfield",
          "defaultValue": "",
          "required": false
        }
      ]
    },
    {
      "name": "open",
      "title": "Open Channel",
      "subtitle": "Quick Slack",
      "description": "Open Registered Channel",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}